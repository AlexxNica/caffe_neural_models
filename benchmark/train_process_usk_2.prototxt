train {
  solver: "../net_usk_2out/neuraltissue_solver.prototxt"
  input {
    padding_size: 180
    patch_size: 512
    channels: 3
    labels: 2
    batch_size: 1
    raw_images: "train/raw"
    label_images: "train/labels"
    preprocessor {
      normalization: true
      rotation: true
      mirror: true
      clahe {
        clip: 4.0
      }
      crop {
        imagecrop: 0
        labelcrop: 0
      }
      blur {
        mean: 0.0
        std: 0.1
        ksize: 5
      }
      histeq {
        patch_prior: false
        masking: false
      }
    }
  }
}

process {
  process_net: "../net_usk_2out/neuraltissue_net.prototxt"
  input {
    padding_size: 180
    patch_size: 512
    channels: 3
    labels: 2
    batch_size: 1
    raw_images: "input"
    preprocessor {
      normalization: true
      clahe {
        clip: 4.0
      }
      crop {
        imagecrop: 0
        labelcrop: 0
      }
    }
  }
  filter_output {
    output_filters: false
    output: "usk_filters"
  }
  output {
    format: "tif"
    fp32_out: false
    output: "output"
  }
}

benchmark {
  train_index: 0
  process_index: 0
  output: "usk_benchmark"
  warmup_runs: 2
  bench_runs: 5
}
